{
    "autoload": {
        "psr-4": {
            "DealerGroup\\": "src/",
            "DealerGroup\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^7",
        "squizlabs/php_codesniffer": "^3.2"
    },
    "require": {
        "php": ">=7.2",
        "webmozart/assert": "^1.4"
    },
    "scripts": {
        "phpcs": "phpcs --standard=PSR2 -p src tests || true",
        "phpcbf": "phpcbf --standard=PSR2 src tests"
    }
}
